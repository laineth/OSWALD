@model OSWALD.Models.Chatbot

<div class="row">
    <div class="col-md-12">
        <h1>OSWALD - Interactive Assistant</h1>
        <p>
            OSWALD is an Artificial Intelligence made with ASP .NET MVC and C#.<br />
            As of for now, OSWALD is able to tell you information about me, don't hesitate to ask what you want to know.
        </p>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <form name="Oswin" id="oswin">
            <textarea name="chat_window" id="chat">
            </textarea><br /><br />
            <input type="text" name="user_entry" id="user_entry"><br /><br />
            <button type="submit" class="btn" id="send">Send</button>
            <button type="reset" class="btn btn-danger">Reset</button>
        </form>
    </div>
</div>